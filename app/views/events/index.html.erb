</head>

<body>
<button class="Llink">
  <a href="/families">To Bank</a>
</button>
    <br><br><br>
<div id="calendar" style="width: 300px"></div>


<form method="post" class="create-event" action="/events">
  <%= token_tag(nil) %>
  <input name="event[title]" type='text'>
  <input name="event[start_time]" type="date" id="datepicker">
  <input name="event[end_time]" type="date" id="datepicker2">
  <label class="assign">
      <span>Event for:</span>
      <select class="child-points" name="event[member_id]">
          <option> </option>
        </select>
        <br><br>
  </label>
  <input type="submit" value='Submit'>
</form>
</body>

<script id="child-points" type="text/x-handlebars-template">
  <option value={{id}}>{{name}}</option>
</script>
